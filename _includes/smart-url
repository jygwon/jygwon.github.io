{% assign url = include.url %}{% if url contains '://' %}{{ url }}{% else %}{% assign url = url | remove_first:site.baseurl %}{{ url | relative_url }}{% endif %}
{%- assign url = include.url -%}
{%- assign ch1 = url | slice:0 -%}
{%- if url contains '://' -%}
  {{ url }}
{%- elsif ch1 == '/' -%}
  {%- assign url = url | remove_first:site.baseurl -%}
  {{ url | relative_url }}
{%- else -%}
  {{ url }}
{%- endif -%}